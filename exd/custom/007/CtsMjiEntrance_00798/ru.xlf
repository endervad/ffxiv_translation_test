      <trans-unit id="16" approved="no" xml:space="preserve">
        <source>16</source>
        <target state="translated">TEXT_CTSMJIENTRANCE_00798_Q3_000_005&lt;tab&gt;Участник моей вольной компании.</target>
      </trans-unit>
      <trans-unit id="17" approved="no" xml:space="preserve">
        <source>17</source>
        <target state="translated">TEXT_CTSMJIENTRANCE_00798_Q3_000_010&lt;tab&gt;Член группы.</target>
