  <file original="VoiceMan_05401" datatype="plaintext" source-language="en" target-language="de">
    <body>
      <trans-unit id="0" approved="yes" xml:space="preserve">
        <source>0</source>
        <target state="final">TEXT_VOICEMAN_05401_000010_GAIA&lt;tab&gt;Ich werde die Dunkelheit extrahieren. Mach dich bereit!</target>
      </trans-unit>
      <trans-unit id="1" approved="yes" xml:space="preserve">
        <source>1</source>
        <target state="final">TEXT_VOICEMAN_05401_000020_RYNE&lt;tab&gt;Ja! Es hat funktioniert, Gaia!</target>
      </trans-unit>
      <trans-unit id="2" approved="yes" xml:space="preserve">
        <source>2</source>
        <target state="final">TEXT_VOICEMAN_05401_000030_RYNE&lt;tab&gt;Dann ist all dies vor hundert Jahren passiert?</target>
      </trans-unit>
      <trans-unit id="3" approved="yes" xml:space="preserve">
        <source>3</source>
        <target state="final">TEXT_VOICEMAN_05401_000040_RYNE&lt;tab&gt;Aber ... was hat das nun mit Gaia zu tun?</target>
      </trans-unit>
      <trans-unit id="4" approved="yes" xml:space="preserve">
        <source>4</source>
        <target state="final">TEXT_VOICEMAN_05401_000050_RYNE&lt;tab&gt;Ich weiß, wo wir sind! Die Zellen von Eulmore!</target>
      </trans-unit>
      <trans-unit id="5" approved="yes" xml:space="preserve">
        <source>5</source>
        <target state="final">TEXT_VOICEMAN_05401_000060_RYNE&lt;tab&gt;Was immer uns auch im Weg steht ...</target>
      </trans-unit>
      <trans-unit id="6" approved="yes" xml:space="preserve">
        <source>6</source>
        <target state="final">TEXT_VOICEMAN_05401_000070_RYNE&lt;tab&gt;... wir werden nie die Hoffnung verlieren!</target>
      </trans-unit>
      <trans-unit id="7" approved="yes" xml:space="preserve">
        <source>7</source>
        <target state="final">TEXT_VOICEMAN_05401_000080_EDEN3BOSS3&lt;tab&gt;Sie verdient ... alles Glück ... der Welt ...</target>
      </trans-unit>
      <trans-unit id="8" approved="yes" xml:space="preserve">
        <source>8</source>
        <target state="final">TEXT_VOICEMAN_05401_000090_EDEN3BOSS4&lt;tab&gt;Eden wird als ein Utopia wiedergeboren werden ...</target>
      </trans-unit>
      <trans-unit id="9" approved="yes" xml:space="preserve">
        <source>9</source>
        <target state="final">TEXT_VOICEMAN_05401_000100_EDEN3BOSS4&lt;tab&gt;Eines, das wir für alle Ewigkeit teilen können.</target>
      </trans-unit>
      <trans-unit id="10" approved="yes" xml:space="preserve">
        <source>10</source>
        <target state="final">TEXT_VOICEMAN_05401_000110_RYNE&lt;tab&gt;Sie ist noch immer da drin ... irgendwo ...</target>
      </trans-unit>
      <trans-unit id="11" approved="yes" xml:space="preserve">
        <source>11</source>
        <target state="final">TEXT_VOICEMAN_05401_000120_RYNE&lt;tab&gt;Nein ... Ich werde unsere Zukunft auf keinen Fall aufgeben, Gaia!</target>
      </trans-unit>
      <trans-unit id="12" approved="yes" xml:space="preserve">
        <source>12</source>
        <target state="final">TEXT_VOICEMAN_05401_000130_EDEN3BOSS4&lt;tab&gt;Unser Utopia ... Unser Traum!</target>
      </trans-unit>
      <trans-unit id="13" approved="yes" xml:space="preserve">
        <source>13</source>
        <target state="final">TEXT_VOICEMAN_05401_000140_EDEN3BOSS5&lt;tab&gt;Ich ... werde ... zurückkommen ...</target>
      </trans-unit>
    </body>
